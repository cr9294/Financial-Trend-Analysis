{
    "三费比重":{
        "关键词":["管理费用", "销售费用", "财务费用", "营业收入"],
        "算式": "(管理费用+销售费用+财务费用)/营业收入"
    },
    "资产负债比率":{
        "关键词":["负债合计", "资产总计"],
        "算式": "负债合计/资产总计"
    },
    "营业利润率":{
        "关键词":["营业利润", "营业收入"],
        "算式": "营业利润/营业收入"
    },
    "速动比率":{
        "关键词":["流动资产合计", "预付款项", "存货", "流动负债合计"],
        "算式": "(流动资产合计-预付款项-存货)/流动负债合计"
    },
    "现金比率":{
        "关键词": ["期末现金及现金等价物余额", "流动负债合计"],
        "算式": "期末现金及现金等价物余额/流动负债合计"
    },
    "净利润率":{
        "关键词": ["净利润", "营业收入"],
        "算式": "净利润/营业收入"
    },
    "财务费用率":{
        "关键词": ["财务费用", "营业收入"],
        "算式": "财务费用/营业收入"
    },
    "营业成本率":{
        "关键词": ["营业成本", "营业收入"],
        "算式": "营业成本/营业收入"
    },
    "管理费用率":{
        "关键词": ["管理费用", "营业收入"],
        "算式": "管理费用/营业收入"
    },
    "流动负债比率":{
        "关键词": ["流动负债合计", "负债合计"],
        "算式": "流动负债合计/负债合计"
    },
    "流动比率":{
        "关键词": ["流动资产合计", "资产总计"],
        "算式": "流动资产合计/资产总计"
    },
    "非流动负债比率":{
        "关键词": ["非流动负债合计", "负债合计"],
        "算式": "非流动负债合计/负债合计"
    },
    "毛利率": {
        "关键词": ["营业收入", "营业成本"],
        "算式": "(营业收入-营业成本)/营业收入"
    }
}